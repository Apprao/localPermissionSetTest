    <validationRules>
        <fullName>Cut_Order_and_Stocking_Should_be_100</fullName>
        <active>false</active>
        <description>Cut_Order__c &amp;&amp; Stocking__c both should be 100%</description>
        <errorConditionFormula>OR( 
 NOT(ISBLANK(Cut_Order__c )) &amp;&amp;  NOT(ISBLANK(Stocking__c )) &amp;&amp; (Cut_Order__c + Stocking__c &lt;&gt;  1.00 ),

ISBLANK(Cut_Order__c )&amp;&amp; NOT(ISBLANK(Stocking__c )) &amp;&amp; (Stocking__c  &lt;&gt; 1.00),
  
NOT(ISBLANK(Cut_Order__c ))&amp;&amp; ISBLANK(Stocking__c ) &amp;&amp; (Cut_Order__c &lt;&gt; 1.00)

)</errorConditionFormula>
        <errorMessage>Cut Order and Stocking percentages sum should be 100%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sum_All_Distribution_Percent_Equal_100</fullName>
        <active>false</active>
        <description>calculate sum of all distribution percentage fields and check total is 100. always need to be 100%</description>
        <errorConditionFormula>AND( 
$Profile.Name != &apos;Integration User&apos;, 
$Profile.Name != &apos;System Administrator&apos;, 
OR( 
NOT(ISBLANK(R_Distribution_Carpet__c)), 
NOT(ISBLANK(R_Distribution_Cushion__c)), 
NOT(ISBLANK(R_Distribution_Hardwood__c)), 
NOT(ISBLANK(R_Distribution_Laminate__c)), 
NOT(ISBLANK(R_Distribution_Resilient__c)), 
NOT(ISBLANK(R_Distribution_Tile__c)) 
), 
( 
IF(NOT(ISBLANK(R_Distribution_Carpet__c)), R_Distribution_Carpet__c, 0) + 
IF(NOT(ISBLANK(R_Distribution_Cushion__c)), R_Distribution_Cushion__c, 0) + 
IF(NOT(ISBLANK(R_Distribution_Hardwood__c)), R_Distribution_Hardwood__c, 0) + 
IF(NOT(ISBLANK(R_Distribution_Laminate__c)), R_Distribution_Laminate__c, 0) + 
IF(NOT(ISBLANK(R_Distribution_Resilient__c)), R_Distribution_Resilient__c, 0) + 
IF(NOT(ISBLANK(R_Distribution_Tile__c)), R_Distribution_Tile__c, 0) 
) &lt;&gt; 1.00 
)</errorConditionFormula>
        <errorMessage>Sum of Distribution Carpet, Distribution Cushion, Distribution Hardwood, Distribution Laminate, Distribution Resilient, Distribution Tile must be equal to 100</errorMessage>
    </validationRules>
